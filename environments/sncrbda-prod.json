    "role_mapr": "= 0.9.0",
    "role_nat": "= 0.1.0",
    "role_rsyslog": "= 0.5.0",
    "role_rtfe": "= 0.2.0",
    "rsyslog": "= 2.2.0",
    "selinux": "= 0.9.0",
    "sncr_artifacts": "= 0.1.0",
    "sncr_aws_nat_monitor": "= 0.4.4",
    "sncr_common_base": "= 0.1.2",
    "sncr_logicmon": "= 1.0.0",
    "sncr_mapr": "= 0.10.1",
