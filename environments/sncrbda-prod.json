    "sudo": "= 2.9.0",
    "sysctl": "= 0.7.5",
    "users": "= 2.0.3",
    "windows": "= 1.40.0",
    "yum": "= 3.10.0",
    "yum-amazon": "= 0.2.3",
    "yum-centos": "= 0.4.12",
    "yum-epel": "= 0.7.0",
    "yum-oracle": "= 1.0.3",
    "zip": "= 1.1.0"
  },
  "default_attributes": {
    "blacklist": {
      "etc": {
        "passwd": true
      }
    },
    "chef_client": {
      "config": {
        "chef_server_url": "https://chef-pabet.sncrcorp.net:443/organizations/sncr-bda",
        "https_proxy": "https://chef-proxy.ivpc.prod.cloud.synchronoss.net:3128",
        "no_proxy": "*.amazonaws.com,10.*,download.oracle.com,*.mapr.com,github.com",
        "validation_client_name": "sncr-bda-validator.pem"
      },
      "cron": {
        "environment_variables": "LC_ALL=en_US.UTF-8",
        "hour": "*/1",
        "log_file": "/var/log/chef-client.last"
      }
    },
    "chef_handler_sns": {
      "supports": {
        "exception": true,
        "report": false
      },
      "topic_arn": "arn:aws:sns:us-east-1:149375139707:SNCRBDA-CW-PROD-OpsAlertsTopic-1IR7G05MN6ECT"
    },
    "java": {
      "jdk_version": "8",
        "install_flavor": "oracle",
      "oracle": {
        "accept_oracle_download_terms": true,
        "jce": {
          "enabled": true
        }
      }
    },
    "ntp": {
      "servers": [
        "0.amazon.pool.ntp.org",
        "1.amazon.pool.ntp.org",
        "2.amazon.pool.ntp.org",
        "3.amazon.pool.ntp.org"
      ]
    },
    "openssh": {
      "server": {
        "banner": "/etc/issue",
        "client_alive_count_max": "3",
        "client_alive_interval": "7200",
        "gateway_ports": "clientspecified",
        "host_based_authentication": "no",
        "ignore_rhosts": "yes",
        "log_level": "INFO",
        "max_auth_tries": "5",
        "permit_empty_passwords": "no",
        "permit_root_login": "no",
        "permit_user_environment": "no",
        "protocol": "2",
        "syslog_facility": "AUTHPRIV",
        "x11_forwarding": "no"
      }
    },
      "rsyslog": {
          "server_search": "roles:rsyslog-server AND chef_environment:sncrbda-prod"
      },
    "sncr_artifacts": {
      "yum-centos": false
    },
    "sncr_mapr": {
      "cldb": [
        "mapr101.sncrbda.prod.cloud.synchronoss.net",
        "mapr102.sncrbda.prod.cloud.synchronoss.net",
        "mapr103.sncrbda.prod.cloud.synchronoss.net"
      ],
      "clustername": "bdaprod_batch",
      "cluster_nodes": [
        "mapr101.sncrbda.prod.cloud.synchronoss.net",
        "mapr102.sncrbda.prod.cloud.synchronoss.net",
        "mapr103.sncrbda.prod.cloud.synchronoss.net",
        "mapr104.sncrbda.prod.cloud.synchronoss.net",
        "mapr105.sncrbda.prod.cloud.synchronoss.net",
        "mapr106.sncrbda.prod.cloud.synchronoss.net",
        "mapr107.sncrbda.prod.cloud.synchronoss.net",
        "mapr108.sncrbda.prod.cloud.synchronoss.net"
      ],
      "clustershell": {
        "rpm_url": "http://s3.amazonaws.com/public.qa.sncrbda.sncr/public/mapr/clustershell-1.6-1.el6.noarch.rpm"
      },
      "hs": "mapr101.sncrbda.prod.cloud.synchronoss.net",
      "node": {
        "disks": "/dev/xvdp,/dev/xvdq,/dev/xvdr"
      },
      "node_count": "8",
      "rm": [
        "mapr104.sncrbda.prod.cloud.synchronoss.net"
      ],
      "users_data_bag": "users_prod",
      "version": "5.1.0",
      "ws": [
        "mapr101.sncrbda.prod.cloud.synchronoss.net",
        "mapr106.sncrbda.prod.cloud.synchronoss.net"
      ],
      "zk": [
        "mapr106.sncrbda.prod.cloud.synchronoss.net",
        "mapr107.sncrbda.prod.cloud.synchronoss.net",
        "mapr108.sncrbda.prod.cloud.synchronoss.net"
      ]
    },
    "sncr_sssd": {
      "conf": {
        "sssd": {
          "domains": [
            "ivpc"
          ]
        }
      },
      "domains": {
        "ivpc": {
          "ldap_default_authtok": "jHX3m7SMryIKM5SuuLo6jMiHv",
          "ldap_default_bind_dn": "cn=orcladmin"
        }
      }
    },
    "sncr_zabbix_agent": {
      "conf": {
        "Server": "zabbix-ri101.ivpc.prod.cloud.synchronoss.net,zabbix-ri201.ivpc.prod.cloud.synchronoss.net",
        "ServerActive": "zabbix-ri101.ivpc.prod.cloud.synchronoss.net,zabbix-ri201.ivpc.prod.cloud.synchronoss.net"
      },
      "rpms": [
        [
          "zabbix-2.4.6-1.el6.x86_64.rpm",
          "e0e9d3d2558b8ad32f184442550e084eee5ab7c6b18de42ca85909e9da283800"
        ],
        [
          "zabbix-agent-2.4.6-1.el6.x86_64.rpm",
          "b32b9e729200ee50759417c2cce09e9a6642789e450730e47e57c5802f063619"
        ],
        [
          "fping-3.10-1.el6.rf.x86_64.rpm",
          "d304a3ffa52256cf87084686d06efe824d461b780bf93e2eb4065a8faf13ba41"
        ]
      ],
      "url": "s3://private.prod.sncrbda.sncr/zabbix"
    }
  },
  "description": "Prod environment for EBU BDA",
  "json_class": "Chef::Environment",
  "name": "sncrbda-prod",
  "override_attributes": {
    "authorization": {
      "sudo": {
        "groups": [
          "wheel",
          "sg_unix_bda_dev_sudo"
        ]
      }
    },
    "sncr_sssd": {
      "domains": {
        "ivpc": {
          "ldap_access_filter": "(|(memberOf=CN=sg_unix_bda_dev,OU=Groups,OU=UNIX,DC=synchronoss,DC=net)(memberOf=CN=sg_unix_bda_dev_sudo,OU=Groups,OU=UNIX,DC=synchronoss,DC=net))"
        }
      }
    },
    "snmp": {
      "groups": {
        "v1": {
          "notConfigGroup": []
        },
        "v2c": {
          "notConfigGroup": []
        }
      },
      "sec_name": {
        "notConfigUser": []
      },
      "sec_name6": {
        "notConfigUser": []
      }
    }
  }
}
